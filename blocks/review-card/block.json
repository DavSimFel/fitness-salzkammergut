{
  "apiVersion": 2,
  "name": "fitness/review-card",
  "title": "Testimonial Card",
  "description": "Outputs a curated testimonial with live star rating from Google.",
  "category": "widgets",
  "icon": "format-quote",
  "textdomain": "fitness-skg",
  "attributes": {
    "testimonialId": {
      "type": "integer",
      "default": 0
    },
    "placeId": {
      "type": "string",
      "default": ""
    },
    "showStars": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "align": ["left", "center", "right", "wide"]
  },
  "editorScript": "fitness-skg-google-review-blocks"
}
